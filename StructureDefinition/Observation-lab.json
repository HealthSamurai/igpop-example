{
  "resourceType" : "StructureDefinition",
  "id" : "Observation-lab",
  "url" : "https://healthsamurai.github.io/igpop-example/StructureDefinition/Observation-lab",
  "name" : "Observation",
  "title" : "US Core Observation Profile",
  "status" : "active",
  "experimental" : false,
  "date" : "2021-03-23",
  "publisher" : "US Core",
  "description" : "Laboratory results are grouped and summarized using the DiagnosticReport\nresource which reference Observation resources. Each Observation resource\nrepresents an individual laboratory test and result value, a “nested” panel\n(such as a microbial susceptibility panel) which references other observations,\nor rarely a laboratory test with component result values. This profile sets\nminimum expectations for the Observation resource resource to record, search\nand fetch laboratory test results associated with a patient. It identifies\nwhich core elements, extensions, vocabularies and value sets SHALL be present\nin the resource when using this profile.\n",
  "fhirVersion" : "4.0.1",
  "kind" : "resource",
  "abstract" : false,
  "type" : "Observation",
  "baseDefinition" : "http://hl7.org/fhir/StructureDefinition/Observation",
  "derivation" : "constraint",
  "differential" : {
    "element" : [ {
      "id" : "Observation.status",
      "path" : "Observation.status",
      "mustSupport" : true,
      "min" : 1,
      "binding" : {
        "valueSet" : "https://healthsamurai.github.io/igpop-example/ValueSet/fhir:observation-status",
        "strength" : "extensible",
        "description" : null
      }
    }, {
      "id" : "Observation.category",
      "path" : "Observation.category",
      "mustSupport" : true,
      "min" : 1,
      "contains" : {
        "coding" : [ {
          "system" : "http://terminology.hl7.org/CodeSystem/observation-category",
          "code" : "laboratory"
        } ]
      }
    }, {
      "id" : "Observation.code",
      "path" : "Observation.code",
      "mustSupport" : true,
      "min" : 1,
      "binding" : {
        "valueSet" : "https://healthsamurai.github.io/igpop-example/ValueSet/fhir:observation-codes",
        "strength" : "extensible",
        "description" : null
      }
    }, {
      "id" : "Observation.subject",
      "path" : "Observation.subject",
      "mustSupport" : true,
      "min" : 1,
      "refs" : [ "/Patient" ]
    }, {
      "id" : "Observation.effective",
      "path" : "Observation.effective",
      "mustSupport" : true
    }, {
      "id" : "Observation.value[x]",
      "path" : "Observation.value[x]",
      "mustSupport" : true,
      "type" : [ {
        "code" : "Quantity"
      }, {
        "code" : "CodeableConcept"
      }, {
        "code" : "integer"
      }, {
        "code" : "Range"
      }, {
        "code" : "Ratio"
      } ],
      "Quantity" : { },
      "CodeableConcept" : { },
      "integer" : { },
      "Range" : { },
      "Ratio" : { }
    }, {
      "id" : "Observation.dataAbsentReason",
      "path" : "Observation.dataAbsentReason",
      "mustSupport" : true,
      "binding" : {
        "valueSet" : "https://healthsamurai.github.io/igpop-example/ValueSet/fhir:data-absent-reason",
        "strength" : "extensible",
        "description" : null
      }
    } ]
  }
}